{
  "name": "test-wf",
  "trigger-defs": [
    {
      "name": "test-trigger-1",
      "source": "testsource",
      "eventID": "testeventid",
      "correlation-token": "testcorrelationtoken"
    },
    {
      "name": "test-trigger-2",
      "source": "testsource2",
      "eventID": "testeventid2",
      "correlation-token": "testcorrelationtoken2"
    },
    {
      "name": "test-trigger-3",
      "source": "testsource3",
      "eventID": "testeventid3",
      "correlation-token": "testcorrelationtoken3"
    }
  ],
  "states": [
    {
      "name": "test-state-1",
      "events": [
        {
          "event-expression": "trigger != null && trigger.equals('test-trigger-1')",
          "timeout": "testTimeout",
          "action-mode": "SEQUENTIAL",
          "actions": [
            {
              "function": {
                "name": "testFunction"
              },
              "timeout": 5,
              "retry": {
                "match": "testMatch",
                "retry-interval": 2,
                "max-retry": 10,
                "next-state": "testNextRetryState"
              }
            }
          ],
          "next-state": "testNextState"
        }
      ],
      "type": "EVENT",
      "start": true
    },
    {
      "name": "test-state-2",
      "events": [
        {
          "event-expression": "trigger != null && (trigger.equals('test-trigger-1') || trigger.equals('test-trigger-2'))",
          "timeout": "testTimeout",
          "action-mode": "SEQUENTIAL",
          "actions": [
            {
              "function": {
                "name": "testFunction"
              },
              "timeout": 5,
              "retry": {
                "match": "testMatch",
                "retry-interval": 2,
                "max-retry": 10,
                "next-state": "testNextRetryState"
              }
            }
          ],
          "next-state": "testNextState"
        }
      ],
      "type": "EVENT",
      "start": false
    }
  ]
}